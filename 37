#include <iostream>
using namespace std;

int main()
{
    int a = 5, b = 5, c = 9;
    if (a >= b >= c)
    {
        cout << a * 2 << endl;
        cout << b * 2 << endl;
        cout << c * 2 << endl;
    }
    else
    {
        cout << abs(a) << endl;
        cout << abs(b) << endl;
        cout << abs(c) << endl;
    }
    cin >> a;
}
